<h1>Contact page!!</h1>

<% if params[:member].present? %>
  <p>You searched for <%= @search %></p>
<% end %>

<form action="/contact" method="get">
  <input type="text" name="member" value="<%= params[:member] %>" placeholder="Who are you looking for?">
  <input type="submit">
</form>

<h2>Meet our team!</h2>
<ol>
  <% @members.each do |member| %>
  <li>
    <%= member.capitalize %>
  </li>
  <% end %>
</ol>


